<%- include('includes/header',  {username: admin[0].name,email: admin[0].email } ) %>

  <div class="body-wrapper">
    <div class="container-fluid">
      <div class="card bg-info-subtle shadow-none position-relative overflow-hidden mb-4">
        <div class="card-body px-4 py-3">
          <div class="row align-items-center">
            <div class="col-md-8 col-sm-7">
              <h4 class="fw-semibold mb-1">Lead History</h4>
            </div>


          </div>

        </div>
      </div>
    <div class="row">
           <div class="col-md-6">
              <div class="card">
                <div class="card-body">
                  <div class="d-flex flex-row">
                    <div class="round-40 rounded-circle text-white d-flex align-items-center justify-content-center text-bg-success">
                      <i class="fa-solid fa-database"></i>
                    </div>
                    <div class="ms-3 align-self-center">
                      <h4 class="mb-0 fs-5">Total Leads</h4>
                    </div>
                    <div class="ms-auto align-self-center">
                      <h2 class="fs-7 mb-0"><%=totalLeads%></h2>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <!-- Column -->
            <div class="col-md-6">
              <div class="card">
                <div class="card-body">
                  <div class="d-flex flex-row">
                    <div class="round-40 rounded-circle text-white d-flex align-items-center justify-content-center text-bg-info">
                      <i class="fa-solid fa-layer-group"></i>
                    </div>
                    <div class="ms-3 align-self-center">
                      <h4 class="mb-0 fs-5">Top Category</h4>
                    </div>
                    <div class="ms-auto align-self-center">
                      <h2 class="fs-7 mb-0"><%=topCategory%></h2>
                    </div>
                  </div>
                </div>
              </div>
            </div>
    </div>
      <div class="datatables">
        <div class="card">
          <div class="card-body">
            <div class="table-responsive">
              <table id="zero_config" class="table table-striped table-bordered text-nowrap align-middle">
                <thead>
                  <tr>
                    <th>User</th>
                    <th>Category</th>
                    <th>Country</th>
                    <th>State</th>
                    <th>Number of Data</th>
                    <th>Type</th>                   
                    <th>Created At</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                </tbody>
              </table>
            </div>
          </div>
        </div>


      </div>



    </div>
  </div>
  </div>

  <%- include('includes/allscripts') %>
          <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.4/moment.min.js"></script>

<script src="/js/view.js/leadhistory.js"></script>